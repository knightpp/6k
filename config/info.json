{
    "keyboard_name": "6k",
    "manufacturer": "knightpp",
    "url": "",
    "maintainer": "knightpp",

    "processor": "RP2040",
    "bootloader": "rp2040",

    "usb": {
      "vid": "0xFEED",
      "pid": "0x0037",
      "device_version": "0.0.1"
    },

    "bootmagic": {
      "enabled": true,
      "matrix": [0, 0]
    },
    "features": {
      "bootmagic": true,
      "extrakey": true,
      "console": true,
      "command": true,
      "rgblight": true
    },
    "mouse_key": {
      "enabled": true
    },

    "ws2812":{
      "driver": "vendor",
      "pin": "GP23"
    },
    "rgblight": {
      "led_count": 1,
      "animations": {
        "breathing": true,
        "rainbow_mood": true,
        "rainbow_swirl": true,
        "snake": true,
        "knight": true,
        "christmas": true,
        "static_gradient": true,
        "rgb_test": true,
        "alternating": true,
        "twinkle": true
      },
      "default": {
        "animation": "rainbow_mood",
        "on": false
      },
      "sleep": true
    },

    "matrix_pins": {
      "direct": [["GP28", "GP26", "GP16", "GP0", "GP9", "GP15"]]
    },

    "dynamic_keymap": {
      "layer_count": 10
    },

    "layouts": {
      "LAYOUT": {
        "layout": [
          {"label": "k00", "matrix": [0, 0], "x": 0, "y": 0},
          {"label": "k01", "matrix": [0, 1], "x": 1, "y": 0},
          {"label": "k02", "matrix": [0, 2], "x": 2, "y": 0},

          {"label": "k10", "matrix": [0, 3], "x": 0, "y": 1},
          {"label": "k11", "matrix": [0, 4], "x": 1, "y": 1},
          {"label": "k12", "matrix": [0, 5], "x": 2, "y": 1}
        ]
      }
    }
  }
